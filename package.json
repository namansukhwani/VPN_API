{
  "name": "flextream-apiservices",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node ./src/bin/www.ts",
    "watch:dev": "nodemon ./src/bin/www.ts",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "dependencies:clean": "rimraf package-lock.json && rimraf node_modules",
    "dependencies:install": "npm i",
    "build": "npm-run-all clean compile postbuildcopy",
    "compile": "npx tsc",
    "postbuildcopy": "npm-run-all copyPublic copyViews",
    "copyPublic": "copyfiles -u 1 \"./src/public/**\" \"./dist/src/\" ",
    "copyViews": "copyfiles -u 1 \"./src/views/**\" \"./dist/src/\" ",
    "clean": "rimraf dist/*",
    "deploy": "git push heroku main",
    "clean-install-deps": "npm-run-all dependencies:clean dependencies:install",
    "changeDeployRemote:vpn": "heroku git:remote -a vpn-api",
    "changeDeployRemote:flextream": "heroku git:remote -a flextream",
    "changeDeployRemote:flextream2": "heroku git:remote -a flextream2",
    "start:pm2": "pm2-runtime start ecosystem.config.js --env production",
    "install": "npm i "
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "file-type": "^16.3.0",
    "http-errors": "~1.6.3",
    "ioredis": "^4.28.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "pm2": "^5.2.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/ioredis": "^4.28.1",
    "@types/lodash": "^4.14.176",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
